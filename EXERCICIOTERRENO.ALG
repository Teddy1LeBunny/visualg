Algoritmo "exercicioretangulo"

Var

   z, p, x, a, y : real

Inicio

   escreval( "Comprimento do terreno:" )
   leia (x)
   escreval ("Largura do terreno:")
   escreval("Preço do terreno: ")
   leia (z)
   leia (y)
   a <- x*y
   p <- a*z
   escreval ("Area do terreno", a:8:2)
   escreval ("Valor final do terreno: ", z:8:2)

Fimalgoritmo